{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "Logs",
        "Effect": "Allow",
        "Action": [
          "logs:CreateLogGroup",
          "logs:CreateLogStream",
          "logs:PutLogEvents"
        ],
        "Resource": [
          "arn:aws:logs:$CF_LOCAL_AWS_REGION:$CF_LOCAL_AWS_ACCOUNT_ID:log-group:/aws/codebuild/$CF_LOCAL_ENV_ID*",
          "arn:aws:logs:$CF_LOCAL_AWS_REGION:$CF_LOCAL_AWS_ACCOUNT_ID:log-group:/aws/codebuild/$CF_LOCAL_ENV_ID*:log-stream:*"
        ]
      },
      {
        "Sid": "Artifacts",
        "Effect": "Allow",
        "Action": [
          "s3:GetObject",
          "s3:PutObject",
          "s3:GetObjectVersion",
          "s3:DeleteObject",
          "s3:ListBucket"
        ],
        "Resource": [
          "arn:aws:s3:::$CF_LOCAL_AWS_ARTIFACT_BUCKET",
          "arn:aws:s3:::$CF_LOCAL_AWS_ARTIFACT_BUCKET/*"
        ]
      }
    ]
  }
